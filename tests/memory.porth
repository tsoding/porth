include "std.porth"

// write "abc" into the memory
mem 0 + 97 .
mem 1 + 98 .
mem 2 + 99 .
mem 3 + 10 .

// print "abc" to stdout
4 mem stdout write print

// increament each character by 1 making it "bcd"
mem 0 + dup , 1 + .
mem 1 + dup , 1 + .
mem 2 + dup , 1 + .

// print "bcd" to stdout
4 mem stdout write print

// print UINT64_MAX (Largest 64 bit word)
18446744073709551615 mem !64 
mem @64 print
